{
  "name": "universal-boilerplate",
  "version": "0.1.0",
  "description": "Minimmun universal boilerplate",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev-server": "cross-env BABEL_ENV=server NODE_ENV=development nodemon ./src/server/index.js --exec babel-node --ignore dist/ -e js,scss,css",
    "dev-client": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev.js",
    "dev": "concurrently \"cross-env NODE_ENV=development npm run dev-server\" \"cross-env NODE_ENV=development npm run dev-client\"",
    "prod-server": "cross-env BABEL_ENV=server NODE_ENV=production babel src --out-dir ./dist",
    "prod-client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "build": "npm run clean && npm run prod-server && npm run prod-client",
    "start": "cross-env NODE_ENV=production node ./dist/server/index.js"
  },
  "author": "Nadia Trigo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "express",
    "react-router"
  ],
  "dependencies": {
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.4.5",
    "styled-normalize": "^8.0.0",
    "webpack-assets-by-type-plugin": "^0.1.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.0.1",
